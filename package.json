{
    "name": "raspiscript",
    "productName": "raspiscript",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": "src/index.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "lint-ts": "tslint -c tslint.json -p tsconfig.json",
        "lint-es": "eslint src",
        "lint": "yarn run lint-es && yarn run lint-ts",
        "lint-fix": "yarn run lint-es --fix && yarn run lint-ts --fix",
        "start": "yarn run build && electron-forge start",
        "package": "yarn run build && electron-forge package",
        "make": "yarn run build && electron-forge make"
    },
    "keywords": [],
    "author": "marvin",
    "license": "MIT",
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "packageManager": "yarn"
            },
            "electronWinstallerConfig": {
                "name": "raspiscript"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "raspiscript"
            }
        }
    },
    "dependencies": {
        "electron-compile": "^6.4.2"
    },
    "devDependencies": {
        "babel-plugin-transform-async-to-generator": "6.24.1",
        "babel-preset-env": "1.7.0",
        "babel-preset-react": "6.24.1",
        "electron-forge": "5.2.2",
        "electron-prebuilt-compile": "2.0.2",
        "eslint": "3.19.0",
        "eslint-config-airbnb": "15.1.0",
        "eslint-plugin-import": "2.12.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "7.9.1",
        "tslint": "^5.10.0",
        "typescript": "^2.8.3"
    }
}
